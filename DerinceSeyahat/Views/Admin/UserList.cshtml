@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityUser>

@{ 
    ViewData["Title"] = "Kullanıcı Listesi";
}

<section class="about">
    <h1>Kullanıcıların Listesi</h1>
    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <td>Id</td>
                <td>Email</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
            <tr>
                <td>@user.Id</td>
                <td>@user.Email</td>
            </tr>
             }
        </tbody>
    </table>
</section>
